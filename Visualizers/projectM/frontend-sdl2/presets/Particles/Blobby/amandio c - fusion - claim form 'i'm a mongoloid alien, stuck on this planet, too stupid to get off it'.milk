per_frame_8=old_mid_flop=mid_flop;
per_frame_9=chaos=.9+.1*sin(pulse);
per_frame_10=entropy=if(equal(pulse,-20),1+bass_flop+treb_flop+mid_flop+int(rand(2)),entropy);
per_frame_11=bass_thresh = above(bass_att,bass_thresh)*2 + (1-above(bass_att,bass_thresh))*((bass_thresh-1.3)*chaos+1.3);
per_frame_12=bass_flop=abs(bass_flop-equal(bass_thresh,2));
per_frame_13=treb_thresh=above(treb_att,treb_thresh)*2 + (1-above(treb_att,treb_thresh))*((treb_thresh-1.3)*chaos+1.3);
per_frame_14=treb_flop=abs(treb_flop-equal(treb_thresh,2));
per_frame_15=mid_thresh=above(mid_att,mid_thresh)*2 + (1-above(mid_att,mid_thresh))*((mid_thresh-1.3)*chaos+1.3);
per_frame_16=mid_flop=abs(mid_flop-equal(mid_thresh,2));
per_frame_17=bass_changed=bnot(equal(old_bass_flop,bass_flop));
per_frame_18=mid_changed=bnot(equal(old_mid_flop,mid_flop));
per_frame_19=treb_changed=bnot(equal(old_treb_flop,treb_flop));
per_frame_20=bass_residual = bass_changed*sin(pulse*.1*entropy) + bnot(bass_changed)*bass_residual;
per_frame_21=treb_residual = treb_changed*sin(pulse*.1*entropy) + bnot(treb_changed)*treb_residual;
per_frame_22=mid_residual = mid_changed*sin(pulse*.1*entropy) + bnot(mid_changed)*mid_residual;
per_frame_23=pulse=if(above(abs(pulse),20),-20,pulse+(bass_thresh+mid_thresh+treb_thresh)*.032);
per_frame_24=q1=mid_residual;
per_frame_25=q2=bass_residual;
per_frame_26=q3=treb_residual;
per_frame_27=q4=sin(pulse);
per_frame_28=q5=cos(pulse/2+q1);
per_frame_29=q6=above(q1,0)+above(q2,0)+above(q3,0)+above(q3,0)*treb_flop+above(q2,0)*bass_flop+above(q1,0)*mid_flop;
per_frame_30=q7=entropy;
per_frame_31=q8=sin(q6*q1+q7*q2);
per_frame_32=zoom=zoom+.02*q8;
per_frame_33=wave_mystery=sin(q1+q5);
per_frame_34=wave_r=wave_r+.5*sin(q1+q2*2+q4*.14);
per_frame_35=wave_b=wave_b+.5*sin(q2+q3*2+q4*.14);
per_frame_36=wave_g=wave_g+.5*sin(q3+q1*2+q4*.14);
per_frame_37=ob_r=if(bass_flop,ob_r+.5*sin(q1+q3*1.14+q2),wave_b);
per_frame_38=ob_b=if(treb_flop,ob_b+.5*sin(q2+q1*1.14+q3),wave_g);
per_frame_39=ob_g=if(mid_flop,ob_g+.5*sin(q3+q2*1.14+q1),wave_r);
per_frame_40=ib_r=if(bass_flop,ob_b,ib_r+.5*cos(q5+q1*2.14));
per_frame_41=ib_b=if(treb_flop,ob_g,ib_b+.5*cos(q5+q2*2.14));
per_frame_42=ib_g=if(mid_flop,ob_r,ib_g+.5*cos(q5+q3*2.14));
per_frame_43=mv_r=mv_r+.5*sin(q4+q5*1.14*q1);
per_frame_44=mv_b=mv_b+.5*sin(q4+q5*1.14*q2);
per_frame_45=mv_g=mv_g+.5*sin(q5+q5*1.14*q3);
per_frame_46=ob_a=.25+.25*sin(q2+q3*2.14);
per_frame_47=ib_a=.5+.5*sin(q2*2.14+q3);
per_frame_48=mv_a=mv_a+mv_a*sin(q3*2.14+q2);
per_frame_49=ob_size=.1+.1*sin(q3*3+q1);
per_frame_50=ib_size=ib_size*.5+ib_size*.25*sin(q1*3+q3);
per_frame_51=wave_mode=q6+above(q4,0)+above(q5,0);
