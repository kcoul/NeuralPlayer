comp_32=`rs2 = .5*cos(uv*13) - ret1;;
comp_33=`float dots = saturate(.05/length(rs2)) ;
comp_34=`
comp_35=`
comp_36=`ret = 2*ret1.gbr + 21*dots*(.1+3*ret1) ;
comp_37=`ret = ret1*4;
