per_frame_75=ran2 = if (change, rand (100)/100, ran2);
per_frame_76=ran3 = if (change, rand (100)/100, ran3);
per_frame_77=ran4 = if (change, rand (100) >= 30, ran4);
per_frame_78=ran1_ = ran1_ + max(min(ran1-ran1_,0.02),-0.02)*dt*16;
per_frame_79=ran2_ = ran2_ + max(min(ran2-ran2_,0.02),-0.02)*dt*16;
per_frame_80=ran3_ = ran3_ + max(min(ran3-ran3_,0.02),-0.02)*dt*16;
