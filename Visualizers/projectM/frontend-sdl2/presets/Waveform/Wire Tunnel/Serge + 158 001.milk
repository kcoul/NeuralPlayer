per_pixel_38=  dx = v*(cos((-x+y)*n+t) + sin((x+y)*n+t));
per_pixel_39=  dy = v*(cos((x-y)*n-t) + sin((-x-y)*n-t));
per_pixel_40=
per_pixel_41=
per_pixel_42=/////////////////////////////////////////////////////////
