comp_10=`    float2 uv_y2 = (uv_y*1.25)-.125;  
comp_11=`
comp_12=`
comp_13=`    ret = lerp(ret,1.1-ret,.8*GetBlur1(uv_y)+.2*GetPixel(uv));
comp_14=`    ret = lerp(ret,ret-.135*GetBlur3(uv_y)+.125*GetBlur1(uv_y2)+.35*GetPixel(uv_y),.25);
comp_15=`    //ret *= .75;
comp_16=`    ret *= saturate(length(ret.xy-0.5)*5);
comp_17=`    ret = ret.xxy;
comp_18=`    ret += 1.15;
comp_19=`    float3 mask = GetBlur3(uv)-GetBlur1(uv)-0*GetPixel(uv_y);
comp_20=`    ret *= lerp(ret,ret*mask,pow(hue_shader.zxy,ret));
comp_21=`    ret = pow(ret,2.55);
comp_22=`}
