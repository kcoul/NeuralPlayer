warp_4=`float tmp, lprof,lprof2,sprof,sprof2, n, stone,stone_lit,sun,sky;
warp_5=`float hormask, lavmask,ilavmask,cldmask,ncloud;
warp_6=`static float lav_gnd = 0,hordist = 8, tele = 1;
warp_7=`static float2 mov = float2 (q5,q6);
warp_8=`
warp_9=`float hq_n (float3 uvi) {return lum(tex2D (sampler_noise_hq ,uvi.xy));}
warp_10=`float noise2 (float3 uvi) {return tex2D (sampler_noise_lq,uvi*8);}
warp_11=`
warp_12=`float noise4 (float2 uvi) { float n1,n2,n3,n4,zv; zv = 0.006*time;
warp_13=`  n1 = lum(tex2D(sampler_noise_hq, (uvi)));
warp_14=`  n2 = lum(tex2D(sampler_noise_hq, (uvi*2+zv)));
warp_15=`  n3 = lum(tex2D(sampler_noise_hq, (uvi*4+zv)));
warp_16=`  n4 = lum(tex2D(sampler_noise_hq, (uvi*8+4*zv)));
warp_17=`  return n1 + n2/2 + n3/4 + n4/8;}
warp_18=`
warp_19=`float noise3 (float3 uvi) {float arg;
warp_20=`return  clamp(tex2D (sampler_noise_hq,uvi.xy/16)-.5,-.25,.25)*2
warp_21=`        +(tex2D (sampler_noise_hq,uvi.xy/8)-.5)/4
warp_22=`        +(tex2D (sampler_noise_hq,(uvi.x-sin(uvi.y*2))/2))/12;}
warp_23=`
warp_24=`float3 rs_lav (float2 uvi, float berg) {  float z; float2 rs;
warp_25=`  z = 8/(uvi.y)*(q8-4*berg); z = clamp(z,-8,80*hordist); //#final check !
warp_26=`  rs.x = uvi.x*z ; rs.y = z; 
warp_27=`  rs = mul(rs,float2x2(q3,q4,-q4,q3));  
warp_28=`  return float3(rs/64 + mov,z); } 
warp_29=`
warp_30=`float3 rs_sky (float2 uvi, float berg) {  float z; float2 rs;
warp_31=`  z = 8/(uvi.y - berg-.5); z = clamp(z,-32,1);
warp_32=`  rs.x = (uvi.x)*z ; rs.y = z; 
warp_33=`  rs = mul(rs,float2x2(q3,q4,-q4,q3));
warp_34=`  return float3(-rs/64+mov/-128,z); }
warp_35=`
warp_36=`shader_body {
warp_37=`uv1 = uv*aspect.xy-float2(.5,q7);
warp_38=`uv1 = mul(uv1,float2x2(q1,q2,-q2,q1));
warp_39=`screen3 = float3 (float2(uv.x*2-1,uv.y-.5)*aspect.yx,1);
warp_40=`
warp_41=`//CLOUDS
warp_42=`uv2 = rs_sky(uv1,0);tmp = 0;
warp_43=`for (int n = 1; n <= 3; n++) {
warp_44=`  uv2 = rs_sky(uv1,tmp*.03);
warp_45=`  tmp = saturate(lum(tex2D (sampler_noise_hq,uv2 +.03*noise4(uv2*2))-.5));}
warp_46=`tmp = saturate(pow(tmp,1.4) * sign(tmp));
warp_47=`cloud = tmp;
warp_48=`ncloud = saturate(1-tmp);
warp_49=`
warp_50=`//DUNES
warp_51=`for (n=1; n <= quality; n++) {
warp_52=`  rsl = rs_lav(tele*uv1,lav_gnd); //#spiele an uv-Vorfakt - zoom, auch bei sky !
warp_53=`  lprof = noise3(rsl);
warp_54=`  lav_gnd -= lprof/quality*depth/sqrt(rsl.z); } 
warp_55=`rsl = rs_lav(tele*uv1,lav_gnd); 
warp_56=`stone = noise2(rsl);
warp_57=`
warp_58=`float3 rsl3 = rsl + float3(-.1,.0,0);
warp_59=`lprof2 = noise3(rsl3);
warp_60=`stone_lit = .3+4*saturate(.2+lprof-lprof2);
warp_61=`
warp_62=`//------------------- masking -----------------------------------------
warp_63=`lavmask = saturate (4*hordist-rsl.z/18) * (rsl.z >= 0); // # !
warp_64=`ilavmask = saturate(1-lavmask);
warp_65=`
warp_66=`//---------------- skylight first-----------------
warp_67=`ret1 = ncloud*uv.y*float3 (.3,.4,1); 
warp_68=`ret1 += cloud*2;
warp_69=`ret1 *= ilavmask;  
warp_70=`
warp_71=`//----------------lava / stone  -----------------
warp_72=`ret1 += stone *stone_lit*lavmask * float3(1,.4,.0);
warp_73=`
warp_74=`
warp_75=`ret = 1-exp(-ret1*2+.1+.1*sin(time/7)); 
warp_76=`
warp_77=`float4 noise9 = tex3D(sampler_noisevol_hq, 
warp_78=`                      ((float3(uv1.y-uv2.x,uv2.x+uv1.x,uv.x*uv.y)*q27
warp_79=`                       )*texsize.xyy*texsize_noisevol_hq.zww
warp_80=`                      ).xyz*
warp_81=`                      float3(1,1,0)*0.05 + 
warp_82=`                      time*float3(0,0,1)*q29
warp_83=`                     );
warp_84=`if(ret.x> q21*roam_cos.y && ret.x<= q24*roam_sin.x)ret.y -= (noise9).x*.5; 
warp_85=`if(ret.y> q22*roam_sin.x && ret.y<= q25*roam_cos.x)ret.z -= (noise9).y*.5; 
warp_86=`if(ret.z> q23*roam_cos.x && ret.z<= q26*roam_sin.y)ret.x -= (noise9).z*.5; 
warp_87=`}
