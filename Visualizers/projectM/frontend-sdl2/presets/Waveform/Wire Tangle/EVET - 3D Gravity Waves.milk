per_frame_1=dec_med = pow (0.9, 30/fps); dec_slow = pow (0.98, 30/fps);
per_frame_2=beat = bass+mid+treb;
per_frame_3=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_4=is_beat = above(beat, .2+avg+peak) * above (time, t0+.1);
per_frame_5=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_6=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_7=index = (index + is_beat) %16;
per_frame_8=index2 = (index2 + is_beat*bnot(index))%12;
per_frame_9=
per_frame_10=spb = min(t0-t0a + .01,2); t0a = t0;
per_frame_11=spb_ = if(is_beat,spb_ *.9 +.1*spb, spb_);
per_frame_12=mindev = min(min(abs(spb-spb_),abs(spb*2-spb_)),abs(spb-spb_*2));
per_frame_13=sdev = if(is_beat,sdev * .9 + .1*pow(mindev+.02,2),sdev);
per_frame_14=
per_frame_15=q20 = avg; q21 = beat;
per_frame_16=q22 = peak+.1; 
per_frame_17=q24 = is_beat;
per_frame_18=q27 = index; q28 = index2;
per_frame_19=
per_frame_20=trel = trel + 1/fps/spb_*4; itrel = int(trel);
per_frame_21=q25 =  above(itrel, itrel_);
per_frame_22=q30 = itrel%2;
per_frame_23=itrel_ = itrel;
per_frame_24=q31 = trel/8-int(trel/8);
per_frame_25=
per_frame_26=ma = ma + peak/fps ;
per_frame_27=mx=mx*dec_med+(1-dec_med)*cos(ma);
per_frame_28=my=my*dec_med+(1-dec_med)*sin(ma+trel/16);
per_frame_29=q15 = mx ; q16 = my ; q17 = ma;
per_frame_30=
per_frame_31=//first par: off; second: on
per_frame_32=sw1 = if(sw1,bnot(q24*bnot(int(rand(10)))),q24*bnot(int(rand(20))));
per_frame_33=sw2 = if(sw2,bnot(q24*bnot(int(rand(40)))),q24*bnot(int(rand(80))));
per_frame_34=sw3 = if(sw3,bnot(q24*bnot(int(rand(10)))),q24*bnot(int(rand(100+10000*sdev))));
per_frame_35=
per_frame_36=//********* variation parameters warp shader
per_frame_37=q6 = sin(time/15)*sin(time/27)*.1; //zz weighting
per_frame_38=q10 = sin(time/8); //ring
per_frame_39=q10 = q10*q10*.1; 
per_frame_40=q11 = 1.5+sin(time/7.8); //zoom uv2
per_frame_41=q12 = if(below(sdev,.02)*bnot(q8),q25,1); //go
per_frame_42=q13 = .5+sin(time/17); //pow noise 
per_frame_43=q19 = sin(time/5.3)*.4; //sunpos 
per_frame_44=
per_frame_45=//********* variation parameters comp shader
per_frame_46=q3 = sw1;//mirror 
per_frame_47=q4 = .02/sdev; //unused ###
per_frame_48=q5 = max(min(4*sin(trel/64)-3,1),0)*.4; //water
per_frame_49=q7 = below(sdev,.004)*(index2%2); //flash
per_frame_50=q2 = abs(q19)*sqrt(q15*q15+q16*q16); //avoid black - frame eq2
per_frame_51=
per_frame_52=trel2 = trel2+.003*q22*q22/fps;
per_frame_53=tmp = sin(trel2);
per_frame_54=tmp = 20*tmp-19;
per_frame_55=q8 =  tmp*above(tmp,0)*(4/max(q22-4,0.2)+4); // dyn zoom
per_frame_56=q26 = above(q22,16)*(q22-16)*2; //# ok???
per_frame_57=
per_frame_58=tmp = index2/6;
per_frame_59=q14 = abs((tmp-int(tmp))-.5)*6+1; //ang multi ####
per_frame_60=
per_frame_61=tmp =sin(trel/4*3.14); //sym rot
per_frame_62=jmp = jmp + above(tmp,.5)*tmp/fps*8*sw2;
per_frame_63=q9 = jmp*equal(q14,1)*below(sdev,0.04)+time*above(sin(q17/23),.9);
per_frame_64=q18 = tmp*tmp/2 * bnot(q8) * sw3; //waber
