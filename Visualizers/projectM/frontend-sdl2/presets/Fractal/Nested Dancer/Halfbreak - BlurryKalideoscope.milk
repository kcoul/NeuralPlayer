per_frame_28=sx=cursc;
per_frame_29=sy=1-(cursc-1);
per_frame_30=decay=1-.2*beat;
per_frame_31=ob_a=.2*beat;
per_frame_32=ib_a=.3*beat;
per_frame_33=q6=below(aa%4,2)*if(equal(aa%2,0),1,-1);
per_frame_34=q5=above(aa%4,1)*if(equal(aa%2,0),-1,1);
per_frame_35=wave_x=.5+.03*q7*q6;
per_frame_36=wave_y=.5+.03*q7*q5;
per_frame_37=rot = curang;
