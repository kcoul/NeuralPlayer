per_frame_45=beat=above(dbass,.6*maxdbass)*above(frame-lastbeat,FPS/3);
per_frame_46=sure=if(beat*below(abs(frame-(interval+lastbeat)),FPS/5),
per_frame_47=min(.095+sure,1),beat*(sure-.095)+(1-beat)*sure*.9996);
