comp_14=`if(ret.z> q24*q12         && ret.z<= q21*q13        )ret.y -= (noise9).z*.5; 
comp_15=`    ret=lerp(ret,GetPixel(echo),0.3);
comp_16=`
comp_17=`    //darken
comp_18=`    ret=1-ret;
comp_19=`    ret*=lum(ret);
comp_20=`    
comp_21=`    //blood-palletized
comp_22=`    ret=pow(ret,float3(0.2,1.2,0.8));
comp_23=`    ret=lerp(ret,float3(1,0,0),lum(ret));
comp_24=`    ret*=1.33;
comp_25=`
