warp_23=`                      ).xyz*
warp_24=`                      float3(1,1,0)*0.05 + 
warp_25=`                      time*float3(0,0,1)*q29
warp_26=`                     );
warp_27=`if(ret.x> q21*q13         && ret.x<= q24*q11        )ret.y += (noise9).x*.5; 
warp_28=`if(ret.y> q22*q11         && ret.y<= q25*q12        )ret.z += (noise9).y*.5; 
