comp_1=`sampler sampler_pw_noise_lq;
comp_2=`static float quality = 2, depth = q31;
comp_3=`float3 neu, rsl, rsl2, rsl3, ret1, screen3, back,lcol, sky;
comp_4=`float2 uv0, uv1, uv2, uv4;
comp_5=`float  lprof,lprof2,m,n,dist,cloud,mask, shadow, mask1,lmask,leaves,clshadow,gmask;
comp_6=`static float lav_gnd = 0;
comp_7=`static const float anz = 3;
comp_8=`static float2 mov = float2 (q5,q6);
comp_9=`
comp_10=`static float3 t = float3(q14,q15,q16);
comp_11=`static float3 s = float3(q17,q18,q19);
comp_12=`static float3 ts = -cross(s,t);
comp_13=`static float3 pos = float3 (q11,q12,q13);
comp_14=`
comp_15=`float3 project (float3 pos, float dist) {
comp_16=`  float z; 
comp_17=`  z = dist/mul(cross(ts,-t),screen3) *(1-lav_gnd);
comp_18=`  gmask = (z <= 0)*(z >= -8); 
comp_19=`  return gmask* float3(mul(float2x3(ts,t),cross(pos,screen3))*z/256,-z);}
comp_20=`
comp_21=`float3 lproject (float3 pos, float dist) {
comp_22=`  float z; 
comp_23=`  z = dist/mul(cross(ts,-t),screen3) *(1-lav_gnd) ;
comp_24=`  return (z<=0)*(z>=-4)* float3(mul(float2x3(ts,t),cross(pos,screen3))*z/256,-z);}
comp_25=`
comp_26=`float noise2 (float2 uvi)
comp_27=`{return 1*(tex2D (sampler_noise_hq,uvi) + 0*tex2D (sampler_noise_hq,uvi*4)/4);}
comp_28=`
comp_29=`shader_body {uv0 = uv;
comp_30=`screen3 = float3 ((uv-.5)*aspect.xy,1);//wirkt wie zoom / Tele ###
comp_31=`
comp_32=`//*****leaves**********
comp_33=`lav_gnd = tex2D (sampler_noise_hq,uv)*.03;
comp_34=`rsl3 = lproject(pos,1.5) + float3 (q29,-time/118,0) + float3 (mov,0);
comp_35=`float glmask = tex2D (sampler_pw_noise_lq,rsl3.x*.004) >= .3;
comp_36=`uv2 = rsl3.xy*16;
comp_37=`uv2.y += tex2D (sampler_pw_noise_lq,uv2.x/256).g*-2*time;
comp_38=`float3 noise = 8*tex2D (sampler_pw_noise_lq,uv2/256);
comp_39=`lmask = noise.g >= 7.; lmask *= glmask;
comp_40=`uv2 = frac(uv2);
comp_41=`float lside = sin((noise.b-8)*2*time+q32);
comp_42=`float lsize = 3*(noise.g-6);
comp_43=`leaves = lmask*lsize *(3+frac(4*uv2.x/uv2.y))/4
comp_44=`        -26*length((uv2-.5)*float2(1,2/lside));
comp_45=`lcol = (1+tex2D (sampler_noise_hq,uv2/8+noise))* float3 (.8+lside/4,.5,0);;
comp_46=`lcol -= tex2D (sampler_noise_hq,uv2+noise).r*.25;
comp_47=`leaves = saturate(leaves);
comp_48=`
comp_49=`//**** main part ******
comp_50=`lav_gnd = 0;
comp_51=`rsl = project (pos, 1)+float3 (time*.006,.6,0);
comp_52=`back = saturate(1-rsl.y);
comp_53=`
comp_54=`float3 back7=back;
comp_55=`if(frame%3==0)back.z=.71-back7.x;
comp_56=`
comp_57=`cloud = saturate(noise2(rsl.xy/8*float2(1,4))-.4)*saturate(.5+back);
comp_58=`back *= slow_roam_sin.wyx*roam_cos.wzy*.6;//float3(.7,.77,.96);
comp_59=`
comp_60=`if(frame%5==0)back.x=.71-back7.y;
comp_61=`if(frame%7==0)back.y=.71-back7.z;
comp_62=`
comp_63=`sky = lerp(back,2*cloud,cloud/2);
comp_64=`//sky = back;
comp_65=`//sky = back + cloud/4;
comp_66=`ret1 = sky;
comp_67=`lprof2 = 0;
comp_68=`for (n=anz; n >= 1; n--) {
comp_69=`  dist = (n); lav_gnd = 0;  mov += n/anz;
comp_70=`  for (m=1; m <= quality; m++) {
comp_71=`    rsl = project (pos, dist)+float3 (mov,0);
comp_72=`    lprof =  .5-lum(GetBlur1(frac(rsl.xy))); //mittelwert null !#
comp_73=`    lav_gnd -= lprof/quality*depth/rsl.z; 
comp_74=`  }
comp_75=`
comp_76=`  rsl = project (pos, dist)+float3 (mov,0) ; 
comp_77=`  rsl2 = project (pos, dist)+float3 (0,0,0); 
comp_78=`  int first = (n==1);
comp_79=`  if (!first) {shadow = saturate(4*GetPixel(frac(rsl2.xy))) ;}
comp_80=`  neu = GetPixel(frac(rsl.xy));
comp_81=`  float  dista = length(rsl.xy)*.001;
comp_82=`  mask = saturate(16/dist*lum(neu))*gmask;
comp_83=`  neu *= 1-.5*lum(tex2D (sampler_noise_lq,rsl.xy*4));
comp_84=`  clshadow = clamp (q20*1.6-noise2(rsl.xy*float2(.02,.1)+float2(time*.01,0)),.2,1);
comp_85=`  neu *= (1-shadow*.5* (!first))*clshadow;
comp_86=`
comp_87=`//  neu *= saturate(noise2(rsl.xy/16+time*.01)+q20-.5+0*(rsl.z)) ; // Abdunkelung hinten
comp_88=`  neu = lerp(back, neu, saturate(5-rsl.z)); //Nebel
comp_89=`    ret1 = lerp(ret1,neu,mask) ;
comp_90=`}
comp_91=`
comp_92=`rsl = project (pos, dist)+float3 (mov+lprof/2,0); 
comp_93=`
comp_94=`ret1 = lerp(ret1,leaves*(1-shadow/2)*clshadow*lcol, leaves*.5*saturate(!mask));
comp_95=`//ret1 *= clshadow;
comp_96=`//ret = 1-exp(-ret1*2+.1) ;
comp_97=`ret = ret1;
comp_98=`
comp_99=`
comp_100=`}
