per_frame_45=// Flexi's beat detection
per_frame_46=vol = bass*8 + mid*4 + treb*2;
per_frame_47=vol = vol*above(vol,17);
per_frame_48=monitor = vol;
per_frame_49=beat = above(vol,res);
per_frame_50=diff = (1-beat)*diff + beat*(vol-res);
per_frame_51=res = beat*(vol+2*diff) + (1-beat)*(res - (diff*0.04 + 0.12)*60/fps);
per_frame_52=res = max(0,res);
per_frame_53=
per_frame_54=z = max(0,if(beat,z+1.6,z-30/fps));
