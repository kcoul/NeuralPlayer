comp_17=`texsize.zxyy*texsize_noisevol_hq.xzww).xyz*float3(1*roam_sin.x,1*roam_cos.y,0)*0.05
comp_18=`                                        + time*float3(0,0,1*roam_sin.z)*0.2 );
comp_19=`float4 noise2 = tex3D(sampler_noisevol_hq, 
