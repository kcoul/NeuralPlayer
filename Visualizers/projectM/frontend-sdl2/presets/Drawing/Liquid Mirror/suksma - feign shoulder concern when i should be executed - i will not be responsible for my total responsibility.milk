comp_21=`texsize.zxyy*texsize_noisevol_hq.xzww).xyz*float3(1*roam_cos.x,1*roam_sin.z,0)*0.05
comp_22=`                                        + time*float3(0,0,1*roam_cos.y)*0.2 );
