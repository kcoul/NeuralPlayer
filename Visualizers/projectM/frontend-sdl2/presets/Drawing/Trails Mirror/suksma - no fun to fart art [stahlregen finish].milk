comp_17=`
comp_18=`    float2 uv_echo2; 
comp_19=`    uv_echo2.y = (uv.x - 0.5)*(-1) + 0.5;
comp_20=`    uv_echo2.x = uv.y;
comp_21=`
comp_22=`    float3 ret3;
comp_23=`    ret3 = tex2D(sampler_main, uv_echo2 ).xyz*4;
comp_24=`    ret3 -= GetBlur1(uv_echo2)*3;
comp_25=`
comp_26=`    float2 uv_echo3; 
comp_27=`    uv_echo3 = (uv - 0.5)*float2(-1,-1) + 0.5;
comp_28=`    
comp_29=`    float3 ret4;
comp_30=`    ret4 = tex2D(sampler_main, uv_echo3 ).xyz*4;
comp_31=`    ret4 -= GetBlur1(uv_echo3)*3;
comp_32=`
comp_33=`    ret = lerp(0.5*lerp(ret3,ret4,.5),0.5*lerp(ret,ret2,.5),.5);
comp_34=`
comp_35=`
comp_36=`    // little bit of noise:
comp_37=`    ret *= 1 + 0.12*(tex2D(sampler_noise_lq, rand_frame.xy 
comp_38=`           + uv*texsize.xy*texsize_noise_lq.zw).xxx*2-1);
comp_39=` 
comp_40=`    // palettize:
comp_41=`    ret = pow(ret, float3(0.5, 0.5, 0.7));
comp_42=`
comp_43=`    ret = (ret-0.1)*1.1;
comp_44=`   
