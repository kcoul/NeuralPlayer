per_frame_6=
per_frame_7=vol = (bass_att + treb_att + mid_att)*.333333;
per_frame_8=
per_frame_9=ra = 1/tic*.25;
per_frame_10=
per_frame_11=treb_avg = tic*(treb_avg*(1/tic - ra) + ra*treb);
per_frame_12=mid_avg = tic*(mid_avg*(1/tic - ra) + ra*mid);
per_frame_13=bass_avg = tic*(bass_avg*(1/tic - ra) + ra*bass);
per_frame_14=vav = tic*(vav*(1/tic - ra) + ra*(bass+treb+mid)*.33333);
per_frame_15=
per_frame_16=tt = tt + tic*treb;
per_frame_17=mt = mt + tic*mid;
per_frame_18=bt = bt + tic*bass;
per_frame_19=vt = vt + tic*(bass*bass + mid*mid + treb*treb)*.3333;
per_frame_20=
per_frame_21=zoom = 1;
per_frame_22=
per_frame_23=
per_frame_24=ti = time*3;
per_frame_25=ib_r = .5 + .5*sin(ti*1.123);
per_frame_26=ib_g = .5 + .5*sin(ti*1.456);
per_frame_27=ib_b = .5 + .5*sin(ti*1.789);
per_frame_28=
per_frame_29=
per_frame_30=q1 = 1 - vav*.5;
per_frame_31=
per_frame_32=q7 = .5 + .3*sin(vt);
per_frame_33=q8 = .5 + .3*sin(vt*2.6546);
per_pixel_1=mx = (x-.5)*q1 + .5;
per_pixel_2=
per_pixel_3=mx = mx - q7;
per_pixel_4=my = y - q8;
per_pixel_5=mx = mx + equal(mx,0)*.000001;
per_pixel_6=
per_pixel_7=ra = pow(mx*mx + my*my*.5625,.5)*4 + .1;
per_pixel_8=
per_pixel_9=zm = ra*ra + .0001;
per_pixel_10=zm = 1 - 1/zm;
per_pixel_11=dx = zm*(x-q7);
per_pixel_12=dy = zm*(y-q8);
warp_1=`shader_body
warp_2=`{
warp_3=`    // sample previous frame
warp_4=`    ret = tex2D( sampler_fc_main, uv ).xyz*.8;
warp_5=`    ret += GetBlur1(uv)*.2;
warp_6=`
warp_7=`    // darken (decay) over time and distance from center
warp_8=`    ret *= ret/(1.4*rad); //or try: ret -= 0.004;
warp_9=`}
comp_1=`shader_body
comp_2=`{
comp_3=`
comp_4=`uv = 0.5 + (uv-0.5)*0.98;
comp_5=`    float2 d = texsize.zw*4;
comp_6=`    float3 dx = ( GetBlur1(uv+float2(1,0)*d)-GetBlur1(uv-float2(1,0)*d) );
comp_7=`    float3 dy = ( GetBlur1(uv+float2(0,1)*d)-GetBlur1(uv-float2(0,1)*d) );
comp_8=`
comp_9=`float4 noise = tex3D(sampler_noisevol_hq, ((uv.xyy+float3(lum(dx),lum(dy),0)*0.5)*texsize.xyy*texsize_noisevol_hq.zww).xyz*float3(1,1,0)*0.05
comp_10=`                                        + time*float3(0,0,1)*0.2 );
comp_11=`uv -= float2(lum(dx),lum(dy))*0.04;
comp_12=`ret = abs(GetPixel(uv)+GetBlur1(uv)-GetBlur2(uv)-GetBlur3(uv));
comp_13=`ret *= lum(noise)*3; 
comp_14=`ret = pow(ret,0.5);
comp_15=`ret-=roam_sin.wzy*roam_cos.zxy;
comp_16=`}
