warp_2=`{
warp_3=`  float2 uv2 = lerp(uv_orig, uv, 0.2);
warp_4=`
warp_5=`  // diffused version:
warp_6=`  float ss = 9;
warp_7=`  float3 blur = 0.25*(
warp_8=`    tex2D(sampler_main, uv2+ss*texsize.zw*float2( 1, 1)).xyz +
warp_9=`    tex2D(sampler_main, uv2+ss*texsize.zw*float2(-1, 1)).xyz +
warp_10=`    tex2D(sampler_main, uv2+ss*texsize.zw*float2( 1,1)).xyz +
warp_11=`    tex2D(sampler_main, uv2+ss*texsize.zw*float2(-1,-1)).xyz +
warp_12=`    tex2D(sampler_main, uv2+ss*texsize.zw*float2(1,1)).xyz +
warp_13=`    tex2D(sampler_main, uv2+ss/texsize.zw*float2(-1,1)).xyz
warp_14=`)
warp_15=`;
warp_16=`
warp_17=`  // diffusion:  
warp_18=`  ret = tex2D(sampler_main,uv2);//blur;
warp_19=`
warp_20=`  float F2 = 0.035;
warp_21=`  float k2 = 0060;
warp_22=`  float s = 0.325;
warp_23=`  float scale = 7;
warp_24=`
warp_25=`  // reaction:
warp_26=`  float3 z = .89 * ret + sin(.4 * ret);
warp_27=`  float uvv = ret.x*ret.y*ret.y;
warp_28=`  z.x += (-uvv + F2*(1-ret.x) + 0.0007)*scale;
warp_29=`  z.y += (uvv - (F2+k2)*ret.y)*scale;
warp_30=`  //z.x += s*(16-ret.x*ret.y);
warp_31=`  //z.y += s*(ret.x*ret.y - ret.y - ret.z);
warp_32=`  z.z -= 0.02;
warp_33=`
warp_34=`  // apply laplacian:
warp_35=`  z.x += (blur - GetPixel(uv2))*0.7*scale;
warp_36=`
warp_37=`  ret = z;  
warp_38=`  //ret *= 0.98 + 0.02*saturate( (0.75 - rad )*10 )^2;
warp_39=`  //ret *= 0.99tan(saturate(cos(ret)));
warp_40=`
warp_41=`  // add noise:
warp_42=`  ret.x += 0.09*sin((tex2d(sampler_noise_lq, uv*texsize.xy*texsize_noise_lq.zw + rand_frame.xy).xyz*2-1));
warp_43=`
warp_44=`  // darken (decay) over time
warp_45=`  //ret = (ret - 0.2)*0.99;
warp_46=`}
